{
  "name": "{{ pluginName }}",
  "version": "1.0.0",
  "intro": "{{ intro }}",
  "description": "{{ description }}",
  "examplePictures": [
    "./assets/example/1.png"
  ],
  "pluginID": "{{ pluginDirName }}",
  "developer": {
    "id": "{{ devId }}",
    "secret": "{{ devSecret }}"
  },
  "icon": "./assets/icon.png",
